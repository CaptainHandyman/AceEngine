template <typename T>
vector2<T>::vector2() {}

template <typename T>
vector2<T>::vector2(T x, T y)
{
    this->x = x;
    this->y = y;
}

template <typename T>
vector4<T>::vector4() {}

template <typename T>
vector4<T>::vector4(T x, T y,
                    T w, T h)
{
    this->x = x;
    this->y = y;
    this->w = w;
    this->h = h;
}